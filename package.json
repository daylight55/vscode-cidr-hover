{
  "name": "vscode-cidr-hover",
  "displayName": "CIDR Hover Info",
  "description": "Display IP range and number of IPs when hovering over CIDR notation",
  "version": "0.0.2",
  "publisher": "daylight55",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daylight55/vscode-cidr-hover.git"
  },
  "bugs": {
    "url": "https://github.com/daylight55/vscode-cidr-hover/issues"
  },
  "homepage": "https://github.com/daylight55/vscode-cidr-hover#readme",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Formatters"
  ],
  "keywords": [
    "cidr",
    "ip",
    "network",
    "subnet",
    "hover"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "type": "commonjs",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.10.3",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
